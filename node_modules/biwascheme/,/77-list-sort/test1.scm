; 実際にソートしてみて、ちゃんと安定ソートできてるか確かめる

(dotimes (i 30 #f)
  (define n (random-integer 1000))
  (define v (vector-map (lambda (i) (vector (random-integer 12345) i))
                        (list->vector (iota n))))
  ;(print v)
  (define v2 (js-invoke v "sort" (js-closure
                               (lambda (a b)
                                 (- (vector-ref a 0) (vector-ref b 0))))))
  (vector-sort! (lambda (a b) (< (vector-ref a 0) (vector-ref b 0))) v)
  (print (equal? v v2)))

